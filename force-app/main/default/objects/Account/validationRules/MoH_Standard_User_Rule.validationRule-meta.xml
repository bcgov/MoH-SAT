<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MoH_Standard_User_Rule</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISCHANGED(Name), $Profile.Name == &apos;MoH Standard User&apos;,NOT(ISNEW()))</errorConditionFormula>
    <errorMessage>You don&apos;t have permission to edit the Account Name.</errorMessage>
</ValidationRule>
