<?xml version="1.0" encoding="UTF-8"?>
<!--* 
* @Author: Suman 
* @Date: 10 Dec 2023
* @Description: The purpose of this LWC is to Fetch EDRD Patient Account information in Create EDRD Case Lightning flow.
-->
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>58.0</apiVersion>
    <isExposed>true</isExposed>
    <targets>
        <target>lightning__FlowScreen</target>
    </targets>
    <targetConfigs>
        <targetConfig targets="lightning__FlowScreen">
            <property name="Name" type="String" label=""/>
            <property name="Birthdate" type="String" />
            <property name="AccountId" type="String"/>
            <property name="Zipcode" type="String" />
            <property name="state" type="String" />
            <property name="city" type="String" />
            <property name="Country" type="String" />
            <property name="street" type="String" />
            <property name="patientPHN" type="String" />
            <property name="accountPHN" type="String" />
            <property name="PostalCode" type="String" />
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>