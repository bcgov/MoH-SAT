<?xml version="1.0" encoding="UTF-8"?>
<!--* 
* @Author: Suman 
* @Date: 10 Dec 2023
* @Description: The purpose of this LWC is to Fetch Provider Account information in Create EDRD Case Lightning flow.
-->
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>60.0</apiVersion>
    <isExposed>true</isExposed>
    <targets>
        <target>lightning__FlowScreen</target>
    </targets>
    <targetConfigs>
        <targetConfig targets="lightning__FlowScreen">
            <property name="Name" type="String" label=""/>
            <property name="providerPHN" type="String" />
            <property name="ProviderAccId" type="String"/>
            <property name="Type" type="String"/>
            <property name="ProviderIdentifier" type="String"/>
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>